<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.feedeo" default-lazy="false">
  <class name="ArticleProperties" table="Properties">
  	<id name="id" column="idProperty">
  		<generator class="native"/>
  	</id>
  	<property name="alreadyRead"/>
  	<property name="important"/>
  	<many-to-one name="owner" column="idUser"/>
  </class>
</hibernate-mapping>