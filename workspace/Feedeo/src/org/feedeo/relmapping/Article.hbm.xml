<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.feedeo" default-lazy="false">
  <class name="Article">
  	<id name="id" column="idArticle">
  		<generator class="native"/>
  	</id>
  	<property name="title"/>
  	<property name="link"/>
  	<property name="date"/>
  	<property name="content" type="text"/>
  	<many-to-one name="sourceFeed" column="idFeed" class="Feed" not-null="true" lazy="proxy"/>
  	<property name="summary" type="text"/>
  	<property name="author"/>
  	
  </class>
</hibernate-mapping>