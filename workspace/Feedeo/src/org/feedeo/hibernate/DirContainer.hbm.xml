<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.feedeo" default-lazy="false">
  <class name="DirContainer" abstract="true">
    <id name="id" column="idDirContainer">
      <generator class="hilo" />
    </id>
    <set cascade="all-delete-orphan" name="directories" lazy="true">
      <key column="idParent"/>
      <one-to-many class="Directory" />
    </set>
  </class>
</hibernate-mapping>