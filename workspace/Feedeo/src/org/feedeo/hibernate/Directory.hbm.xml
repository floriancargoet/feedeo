<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.feedeo" default-lazy="false">
  <union-subclass name="Directory" extends="DirContainer"><!--
    <key column="idDirContainer"/>
  	<id name="id" column="idDirectory">
  		<generator class="native" />
  	</id>
   --><property name="title"/>
    <set cascade="all" name="articles" lazy="true">
      <key column="idDirectory"/>
      <many-to-many column="idArticle" class="Article"/>
    </set>
    <many-to-one name="owner" column="idUser" />
  	
  </union-subclass>
</hibernate-mapping>