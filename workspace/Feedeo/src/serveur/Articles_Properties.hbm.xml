<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 <hibernate-mapping>
 <class name="serveur.Articles_Properties" table="User_Article">
 
  <composite-id name="idUserArticle" class="serveur.Article_PropertiesPK" >
              <key-many-to-one name="idUser" class="serveur.User" column="idUser" />
              <key-many-to-one name="idArticle" class="serveur.Article" column="idArticle" />
  </composite-id>
  
  <property name="lu" column="lu"/>
  <property name="important" column="important"/>
</class>
</hibernate-mapping>