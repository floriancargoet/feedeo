<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 <hibernate-mapping>
 

 <class name="serveur.UserPreference" table="userpreference">
  <composite-id name="idUserPreference" class="serveur.UserPreferencePK" >
              <key-many-to-one name="IdUser" class="serveur.User" column="IdUser" />
              <key-many-to-one name="IdPreference" class="serveur.Preference" column="IdPreference" />
  </composite-id>
  
  <property name="value" column="value"/>
</class>
</hibernate-mapping>