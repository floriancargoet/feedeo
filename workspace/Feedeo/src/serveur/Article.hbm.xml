        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="feedeo.Article" table="Article">
		<id name="idArticle" column="Id">
			<generator class="increment"></generator>
		</id>
		<property name="title"/>
		<property name="link" />
		<property name="date"  type="timestamp" column="Article_date" />
		<property name="summary"/>
		<property name="read" />
		<property name="author"/>
		
		
